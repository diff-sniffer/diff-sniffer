{
  "name": "diff-sniffer/git",
  "description": "Diff Sniffer for Git",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "license": "MIT",
  "require": {
    "diff-sniffer/core" : "^3.4",
    "php" : "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "phpstan/phpstan": "^0.9"
  },
  "autoload": {
    "psr-4": {
      "DiffSniffer\\Git\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DiffSniffer\\Git\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
