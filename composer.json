{
  "name": "diff-sniffer/git",
  "description": "Diff Sniffer for Git",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "license": "MIT",
  "require": {
    "morozov/diff-sniffer-core" : "^3.1.1",
    "php" : "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "phpstan/phpstan": "^0.9"
  },
  "autoload": {
    "psr-4": {
      "DiffSniffer\\Git\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DiffSniffer\\Git\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
