{
  "name": "diff-sniffer/git",
  "description": "Diff Sniffer, a tool to validate coding standards only in changed line",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/morozov/php-vfs"
    }
  ],
  "require": {
    "ext-json": "*",
    "jean85/pretty-package-versions": "^1.2",
    "morozov/bootstrap": "^1.1",
    "squizlabs/php_codesniffer": "^3.5",
    "php": "^7.3"
  },
  "require-dev": {
    "doctrine/coding-standard": "^7.0",
    "phpunit/phpunit": "^8.5",
    "php-vfs/php-vfs": "1.4.x-dev#16eb61dff29f054345710bce5423bfa1ae7ce9d7",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11"
  },
  "config": {
    "platform": {
      "php": "7.3.0"
    }
  },
  "autoload": {
    "psr-4": {
      "DiffSniffer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DiffSniffer\\Tests\\": "tests/",
      "PHP_CodeSniffer\\" : "vendor/squizlabs/php_codesniffer/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.5.x-dev"
    }
  }
}
