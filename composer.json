{
  "name": "diff-sniffer/git",
  "description": "Diff Sniffer for Git",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "license": "MIT",
  "require": {
    "diff-sniffer/core" : "3.5.x-dev",
    "php" : "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11"
  },
  "config": {
    "platform": {
      "php": "7.2.0"
    }
  },
  "autoload": {
    "psr-4": {
      "DiffSniffer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DiffSniffer\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
